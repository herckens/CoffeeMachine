{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "CoffeeApiCsAws:HelloWorldFunction (dotnetcore3.1)",
            "invokeTarget": {
                "target": "template",
                "templatePath": "CoffeeApiCsAws/CoffeeApiCsAws/template.yaml",
                "logicalId": "HelloWorldFunction"
            },
            "lambda": {
                "payload": {},
                "environmentVariables": {}
            }
        },
        {
            "name": "Attach to Java Functions",
            "type": "java",
            "request": "attach",
            "hostName": "127.0.0.1",
            "port": 5005,
            "preLaunchTask": "func: host start"
        }
    ]
}